ossieName = sinksocket
bindir = $(prefix)/dom/components/sinksocket/cpp/
bin_PROGRAMS = sinksocket

xmldir = $(prefix)/dom/components/sinksocket/
dist_xml_DATA = ../sinksocket.prf.xml ../sinksocket.scd.xml ../sinksocket.spd.xml

distclean-local:
	rm -f config.*
	rm -rf autom4te.cache
	rm -f acinclude.m4
	rm -f aclocal.m4
	rm -f configure
	rm -f depcomp
	rm -f install-sh
	rm -f ltmain.sh
	rm -f Makefile.in
	rm -f missing
	rm -rf .deps

# Sources, libraries and library directories are auto-included from a file
# generated by the REDHAWK IDE. You can remove/modify the following lines if
# you wish to manually control these options.
include $(srcdir)/Makefile.am.ide
sinksocket_SOURCES = $(redhawk_SOURCES_auto)
sinksocket_CXXFLAGS = -Wall $(PROJECTDEPS_CFLAGS) $(BOOST_CPPFLAGS) $(INTERFACEDEPS_CFLAGS) $(redhawk_INCLUDES_auto)
sinksocket_LDADD = $(PROJECTDEPS_LIBS) $(BOOST_LDFLAGS) $(BOOST_THREAD_LIB) $(INTERFACEDEPS_LIBS) $(redhawk_LDADD_auto)
sinksocket_LDFLAGS = -Wall $(redhawk_LDFLAGS_auto)
